generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model FoundingMember {
  id String @id @default(cuid())
  slotNumber Int @unique
  email String
  stripeSubscriptionId String?
  createdAt DateTime @default(now())

  @@map("founding_members")
}
